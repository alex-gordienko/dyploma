{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "no-namespace": false,
        "no-internal-module": false,
        "no-empty-interface": false,
        "ban-types": false,
        "jsx-no-lambda": false,
        "no-shadowed-variable": false,
        "import-name": false,
        "variable-name": false,
        "function-name": false,
        "ordered-imports": false
    },
    "rulesDirectory": []
}
